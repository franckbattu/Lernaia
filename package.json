{
  "name": "lernaia",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start:frontoffice:fr": "ng serve --port 4200 --project=frontoffice",
    "start:frontoffice:en": "ng serve --port 4200 --project=frontoffice --configuration=en",
    "start:backoffice": "ng serve --port 4300 --project=backoffice",
    "test": "ng test",
    "build:user": "ng build --project user --configuration production",
    "build:frontoffice": "ng build --project frontoffice --configuration production --localize && ng run frontoffice:server --configuration production",
    "build:backoffice": "ng build --project backoffice --configuration production",
    "serve:frontoffice": "node dist/frontoffice/server/main.js",
    "extract-i18n:frontoffice": "ng extract-i18n frontoffice --format=xlf2 --output-path projects/frontoffice/src/i18n"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.0.0",
    "@angular/common": "~13.0.0",
    "@angular/compiler": "~13.0.0",
    "@angular/core": "~13.0.0",
    "@angular/forms": "~13.0.0",
    "@angular/platform-browser": "~13.0.0",
    "@angular/platform-browser-dynamic": "~13.0.0",
    "@angular/platform-server": "~13.0.0",
    "@angular/router": "~13.0.0",
    "@nguniversal/express-engine": "^13.0.1",
    "express": "^4.15.2",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.0.4",
    "@angular/cli": "~13.0.3",
    "@angular/compiler-cli": "~13.0.0",
    "@angular/localize": "^13.0.3",
    "@nguniversal/builders": "^13.0.1",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "autoprefixer": "^10.4.4",
    "http-server": "^14.1.0",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^13.0.0",
    "postcss": "^8.4.12",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.0.23",
    "typescript": "~4.4.3"
  }
}
